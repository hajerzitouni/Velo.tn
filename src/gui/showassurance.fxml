<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ShowassuranceController">
   <children>
      <TableView fx:id="table" layoutX="206.0" layoutY="74.0" prefHeight="283.0" prefWidth="323.0">
        <columns>
          <TableColumn fx:id="velo" prefWidth="132.0" text="velo" />
          <TableColumn fx:id="montant" prefWidth="190.0" text="montant" />
        </columns>
      </TableView>
      <Button fx:id="Add" layoutX="185.0" layoutY="485.0" mnemonicParsing="false" onAction="#ajouter" style="-fx-background-color: green; -fx-background-radius: 20;" text="Ajouter" textFill="#f8f8f8">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="update" layoutX="353.0" layoutY="485.0" mnemonicParsing="false" onAction="#update" style="-fx-background-color: blue; -fx-background-radius: 20;" text="Modifier" textFill="#f8f8f8" />
      <Button fx:id="delete" layoutX="544.0" layoutY="485.0" mnemonicParsing="false" onAction="#delete" style="-fx-background-color: red; -fx-background-radius: 20;" text="Effacer" textFill="#f8f8f8" />
      <Label layoutX="21.0" layoutY="34.0" prefHeight="27.0" prefWidth="179.0" text="Liste les assurances">
         <font>
            <Font name="Arial" size="18.0" />
         </font>
      </Label>
      <Button fx:id="retour" layoutX="648.0" layoutY="549.0" mnemonicParsing="false" onAction="#retour" style="-fx-background-color: grey; -fx-background-radius: 20;" text="Retour" textFill="#f8f8f8" />
   </children>
</AnchorPane>
